{
  "name": "openc-server - viewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "openc-server chrome viewer extension",
  "homepage_url": "http://github.com/luiseduardobrito/openc-server",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.min.js",
      "js/socket.io.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "open-c viewer",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "tabs"
  ]
}